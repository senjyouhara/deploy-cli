{
  "name": "@alice-lib/deploy-cli",
  "version": "1.0.10",
  "description": "前端自动化部署脚手架",
  "author": {
    "name": "senjyouhara"
  },
  "files": [
    "bin",
    "es",
    "cjs",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senjyouhara/deploy-cli.git"
  },
  "bugs": {
    "url": "https://github.com/senjyouhara/deploy-cli/issues"
  },
  "type": "module",
  "main": "cjs/index.cjs",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./cjs/index.cjs"
    }
  },
  "bin": {
    "alice-deploy-cli": "bin/deploy-cli.js"
  },
  "scripts": {
    "start": "vite",
    "clean": "rimraf es cjs umd dist",
    "build:esm": "vite build --mode esm",
    "build:cjs": "vite build --mode cjs", 
    "build:min": "vite build --mode min",
    "build:bundle": "vite build --mode bundle",
    "build:all": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:min  && npm run build:bundle",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "prettier": "prettier --write src",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,md,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.ts(x)?": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/ali-oss": "^6.16.2",
    "@types/archiver": "^5.3.1",
    "@types/inquirer": "^8.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@types/node-ssh": "^7.0.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "ali-oss": "^6.17.1",
    "archiver": "5.3.0",
    "axios": "^0.25.0",
    "bundle-require": "^4.0.2",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fast-glob": "^3.3.3",
    "husky": "4.3.8",
    "inquirer": "^7.3.3",
    "joycon": "^3.1.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "11.0.0",
    "minimist": "^1.2.5",
    "node-cmd": "^5.0.0",
    "node-ssh": "^11.0.0",
    "ora": "^5.1.0",
    "precss": "^4.0.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "stylelint": "^15.0.0",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}
